<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/whishlistFragmentTravelBand">
    <fragment
        android:id="@+id/whishlistFragmentTravelBand"
        android:name="myapp.com.cityin.fragment.wish_list_fragment.WishListFragmentTravelBand"
        android:label="fragment_whishlist_fragment_travel_band"
        tools:layout="@layout/fragment_wish_list_fragment_travel_band" >
        <action
            android:id="@+id/action_from_travel_band_to_activities"
            app:destination="@id/whishlistFragmentTravelBandActivities" />
    </fragment>
    <fragment
        android:id="@+id/whishlistFragmentTravelBandActivities"
        android:name="myapp.com.cityin.fragment.wish_list_fragment.WishListFragmentTravelBandActivities"
        android:label="fragment_whishlist_fragment_folder_activities"
        tools:layout="@layout/fragment_wish_list_fragment_folder_activities">
        <argument
            android:name="folderId"
            app:argType="string" />
        <action
            android:id="@+id/action_from_travel_band_to_details_activities"
            app:destination="@id/detailsActivitiesFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailsActivitiesFragment"
        android:name="myapp.com.cityin.fragment.wish_list_fragment.DetailsActivitiesFragment"
        android:label="DetailsActivitiesFragment" >
        <argument
            android:name="activityId"
            app:argType="string" />
    </fragment>
</navigation>